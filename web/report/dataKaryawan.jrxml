     KARYAWAN."NAMA" AS KARYAWAN_NAMA,
     TO_CHAR(KARYAWAN."TGL_MASUK", 'dd-MM-yyyy') AS KARYAWAN_TGL_MASUK,
     KARYAWAN."ALAMAT" AS KARYAWAN_ALAMAT,
     KARYAWAN."GAJI" AS KARYAWAN_GAJI,

